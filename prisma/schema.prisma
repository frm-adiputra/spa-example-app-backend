// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id    String  @id @default(uuid())
  email String  @unique
  name  String?
}

model posts {
  id        String  @id @default(uuid())
  title     String
  content   String?
  published Boolean @default(false)
}

model mataKuliahs {
  id   String @id @default(uuid())
  kode String
  nama String
  sks  Int
}
